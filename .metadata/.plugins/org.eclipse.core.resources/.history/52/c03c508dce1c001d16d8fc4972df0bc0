package view;

import java.awt.BorderLayout;

import javax.swing.Icon;
import javax.swing.ImageIcon;
import javax.swing.JButton;
import javax.swing.JPanel;
import net.miginfocom.swing.MigLayout;

public class PainelInicio extends JPanel {

	private JButton button_adicionar;
	
	public PainelInicio() {
		
		setLayout(new MigLayout("", "[450px]", "[300px]"));
		Icon icone_adicionar = new ImageIcon("plus.png");
		
		button_adicionar = new JButton(icone_adicionar);
		add(button_adicionar);
		
	}

}
